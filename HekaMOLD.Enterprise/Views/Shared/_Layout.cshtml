                    <!--begin::Brand-->
                    <div class="aside-logo flex-column-auto" id="kt_aside_logo">
                        <!--begin::Logo-->
                        <a href="@runningHost">
                            <img alt="Logo" src="~/assets/media/logos/batusan-logo-org.png" class="h-40px logo" />
                        </a>
                        <!--end::Logo-->
                        <!--begin::Aside toggler-->
