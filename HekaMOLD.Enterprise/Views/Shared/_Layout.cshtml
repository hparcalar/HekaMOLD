@using System.Text.RegularExpressions
@using System.Web.Configuration
@using HekaMOLD.Enterprise.Helpers
@using System.Web
@{
    var ctrl = this.ViewContext.RouteData.Values["controller"].ToString();
    var actn = this.ViewContext.RouteData.Values["action"].ToString();
    var rcgr = Request.QueryString["receiptCategory"];
    var runningHost = WebConfigurationManager.AppSettings["RunningHost"];
    var virtualPlantEnabled = WebConfigurationManager.AppSettings["VirtualPlantEnabled"] == "1";
    var showOnlyContent = Request.QueryString["OnlyContent"] == "1";
    var receiptCtg = Request.QueryString.AllKeys.Contains("receiptCategory") ? Request.QueryString["receiptCategory"] : "0";
